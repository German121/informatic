#include <iostream>
using namespace std;
int main()
{ int y;
    cout << "Введите год: ";
    cin >> y;
    if (y % 4 == 0){
         if (y % 100 == 0){
             if (y % 400 == 0)
                cout << y << " Висакосный.";
            else
                cout << y << " Не висакосный.";
         }
       else
            cout << y << " Висакосный.";
    }
    else
        cout << y << " Не висакосный.";
    return 0; }
